{
    "name": "stream-from-the-shell",
    "version": "2.0.0",
    "private": true,
    "description": "Generate an RTMP stream from video files for ingestion by a twitch.tv endpoint",
    "type": "module",
    "scripts": {
        "list": "npx tsc --listFilesOnly | sort",
        "typecheck": "npx tsc && echo 'type checking was successful'",
        "lint": "npx eslint . --ext .tsx,.ts",
        "dev": "NODE_ENV=development TWITCH_ENDPOINT=\"$(cat ../.twitch.endpoint)\" npx tsx --tsconfig ./tsconfig.json src/stream-from-the-shell/main.ts --stream",
        "build": "npx esbuild src/stream-from-the-shell/main.ts --bundle --platform=node --packages=external --format=esm --analyze --outfile=dist/main.js",
        "docker:build": "buildimg=\"$(cat ../docker.registry)/stream-from-the-shell:latest\" && docker build -t \"$buildimg\" ."
    },
    "dependencies": {
        "ascii-table": "^0.0.9",
        "chalk": "^5.3.0",
        "chokidar": "^3.6.0",
        "log-update": "^6.0.0"
    },
    "devDependencies": {
        "@mulekick/eslint-config-muleslint": "^1.0.3",
        "@types/chalk": "^2.2.0",
        "@types/chokidar": "^2.1.3",
        "@types/log-update": "^3.1.0",
        "@types/node": "^20.14.9",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "tsx": "^4.16.0",
        "typescript": "^5.5.3"
    },
    "engines": {
        "node": ">=20.10.0",
        "npm": ">=10.2.4"
    },
    "os": [
        "linux"
    ],
    "author": "mulekick <moolekick@gmail.com> (https://github.com/mulekick)",
    "license": "MIT",
    "repository": {},
    "bugs": {},
    "homepage": "https://github.com/mulekick/stream-from-the-shell"
}